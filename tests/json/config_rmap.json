{
    "input_files": [
        {
            "required": true,
            "allow_multiple": false,
            "name": "genome_fa",
            "value": "<unique_file_id>"
        }

    ],
    "arguments": [{"RE" : {"HindIII" : "A|AGCTT"}
                    }
    ],
    "output_files": [
        {
            "name": "out_dir_rmap",
            "required": true,
            "allow_multiple": false,
            "file": {
                "file_type": ".rmap",
                "meta_data": {
                    "visible": true,
                    "tool": "rmap_tool.py",
