                },
                "file_path": "tests/data/test_run_chicago",
                "data_type": "rmap",
                "compressed": ""
            }
        }
          {
            "name": "Rtree_files",
            "required": true,
            "allow_multiple": false,
            "file": {
                "file_type": "['.idx','.dat']",
