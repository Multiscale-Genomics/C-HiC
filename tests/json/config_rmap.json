                },
                "file_path": ["tests/data/test_rmap/rtree_file.idx", "tests/data/test_rmap/rtree_file.dat"],
                "data_type": "binary",
                "compressed": ""
            }
        }
        {
            "name": "out_prefix_makeRmap",
            "required": true,
            "allow_multiple": false,
            "file": {
                "file_type": ".rmap",
